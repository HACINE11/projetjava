      <ComboBox fx:id="cbMatiere" layoutX="373.0" layoutY="27.0" prefWidth="150.0" promptText="Matiere" />
      <DatePicker fx:id="tfDate" layoutX="23.0" layoutY="70.0" prefHeight="25.0" prefWidth="149.0" promptText="date" />
      <TableView fx:id="Table_emploi" layoutX="121.0" layoutY="163.0" prefHeight="200.0" prefWidth="470.0">
        <columns>
          <TableColumn fx:id="Cclasse" prefWidth="75.0" text="Classe" />
            <TableColumn fx:id="Cmatiere" prefWidth="75.0" text="Matiere" />
            <TableColumn fx:id="Csalle" prefWidth="75.0" text="Salle" />
            <TableColumn fx:id="Cheur_debut" prefWidth="75.0" text="heure_Debut" />
